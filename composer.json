{
    "name": "acmee/ideaworks",
    "type": "project",
    "require": {
        "php": "~7.1",
        "beberlei/DoctrineExtensions": "^1.0",
        "dingo/api": "1.0.*@dev",
        "gedmo/doctrine-extensions": "^2.4",
        "goaop/framework": "^2.1",
        "goaop/goaop-laravel-bridge": "^1.0",
        "graham-campbell/binput": "^3.6",
        "graham-campbell/throttle": "^5.3",
        "laravel-doctrine/extensions": "1.0.*",
        "laravel-doctrine/migrations": "1.1.*",
        "laravel/framework": "^5.4",
        "laravel/tinker": "~1.0",
        "ramsey/uuid-doctrine": "~1.4",
        "rcrowe/twigbridge": "@stable",
        "twig/extensions": "^1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1",
        "filp/whoops": "^2.1",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^6.1",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "autoload": {
        "classmap": [
            "./src/database"
        ],
        "psr-4": {
            "Acme\\": "./src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\Tests\\": "./src/tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('src/.env') || copy('src/.env.example', 'src/.env');\""
        ],
        "post-create-project-cmd": [
            "php ./src/artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php ./src/artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php ./src/artisan optimize"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false,
        "vendor-dir": "src/vendor"
    },
    "minimum-stability": "stable"
}
