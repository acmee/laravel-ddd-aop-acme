{
    "name": "acmee/ideaworks",
    "type": "project",
    "require": {
        "php": "~7.1",
        "beberlei/DoctrineExtensions": "^1.0",
        "belphemur/laravel-gettext": "^5.0",
        "caffeinated/menus": "^3.0",
        "cocur/slugify": "^2",
        "devitek/yaml-configuration": "~3.1",
        "dingo/api": "1.0.*@dev",
        "gedmo/doctrine-extensions": "^2.4",
        "goaop/framework": "^2.1",
        "goaop/goaop-laravel-bridge": "^1.0",
        "graham-campbell/binput": "^3.6",
        "graham-campbell/throttle": "^5.3",
        "jenssegers/mongodb": "~3.2",
        "laravel-doctrine/extensions": "1.0.*",
        "laravel/framework": "^5.4",
        "laravel/tinker": "~1.0",
        "rcrowe/twigbridge": "@stable",
        "twig/extensions": "^1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1",
        "filp/whoops": "^2.1",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^6.1",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Acme\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    },
    "minimum-stability": "stable"
}
